<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.dao.UserMapper">

<select id="search" parameterType="com.example.demo.model.User" 
resultType="com.example.demo.model.User">

select * from users

</select>

<!-- insert 和 apdate 方法不需要填写返回类型 默认是 int -->
<insert id="addOne" parameterType="com.example.demo.model.User">

insert into users values(#{id},#{username},#{password},#{createTime},#{isDeleted}
,#{realName},#{phone},#{roleId})

</insert>


	<!-- <insert id="insert" parameterType="java.util.Map">
		insert into users (id,userName,companyId,roleId)
		values(#{usrID},#{usrName},#{companyId},#{roleId})
	</insert> -->


</mapper>